   "source": [
    "def congruencial (X0, n):\n",
    "    a=3\n",
    "    b=7\n",
    "    m=13\n",
    "    Xi=X0\n",
    "    for i in range(n):\n",
    "        Xi = (a * Xi + b) % m\n",
    "        u=(float(Xi)/float(m))\n",
    "        print u\n",
    "    \n",
    "    return u\n",
    "    \n",
    "congruencial(9, 20)"
   ]
